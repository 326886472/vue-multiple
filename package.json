{
  "name": "mobile-server",
  "type": "module",
  "version": "1.0.3",
  "private": true,
  "packageManager": "pnpm@8.10.2",
  "description": "酵母适用于移动端场景web项目",
  "author": "wenshuang",
  "license": "MIT",
  "homepage": "https://gitlab.foundingaz.cn/cyjiaomu/mobile-server",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.foundingaz.cn/cyjiaomu/mobile-server.git"
  },
  "keywords": [
    "vue3",
    "typescript",
    "vant",
    "mpa",
    "spa",
    "template"
  ],
  "scripts": {
    "dev": "vite --host --mode test",
    "build": "vue-tsc --noEmit && vite build",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "build:audit": "vue-tsc --noEmit && vite build --mode audit",
    "build:prod": "vue-tsc --noEmit && vite build --mode prod",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "lint:css": "stylelint **/*.{css,scss,vue} --fix",
    "lint:js": "eslint . --fix",
    "lint:ls": "ls-lint",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:ls",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "release": "bumpp",
    "new": "node ./scripts/generate.cjs"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@sentry/vue": "^8.2.1",
    "@types/js-cookie": "^3.0.6",
    "@vueuse/core": "^10.5.0",
    "axios": "^1.6.1",
    "dayjs": "^1.11.10",
    "element-plus": "^2.7.3",
    "file-saver": "^2.0.5",
    "fingerprintjs2": "^2.1.4",
    "jquery": "^3.7.1",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "qs": "^6.11.2",
    "vant": "^4.7.3",
    "vue": "^3.3.8",
    "vue-moment": "^4.1.0",
    "vue-router": "^4.3.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.0",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@iconify-json/carbon": "^1.1.21",
    "@ls-lint/ls-lint": "^2.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/mockjs": "^1.0.10",
    "@types/node": "20.9.0",
    "@types/qs": "^6.9.11",
    "@unocss/preset-rem-to-px": "^0.57.2",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.0.2",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-px-to-viewport-8-plugin": "^1.2.3",
    "postcss-scss": "^4.0.9",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "taze": "^0.12.0",
    "terser": "^5.24.0",
    "typescript": "^5.2.2",
    "unocss": "^0.57.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-icons": "^0.17.3",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eruda": "^1.0.1",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^1.8.22"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,vue}": [
      "eslint --fix"
    ],
    "src/**/*.{css,vue,scss}": [
      "stylelint --fix"
    ],
    "src/**": [
      "ls-lint"
    ]
  }
}
